
#include "typedefine.h"
#include "sdif_spi.h"

// -------------------------------------------------------
// ----------------------------- Functions ( subroutines )
//
//  SDIF by SPI  utilities 
//
//  for Inputs
uint8_t SDIF_SPIInit( st_HMI *hmi){
    // reset state & time count of pressing
    for( int i=0 ; HMI_N_SW < i ; i++ ){
      hmi->sw_state[i] = 0;
      hmi->sw_cnt[i]   = 0;
    }
    return( 0 );
}

/* end of sdif_spi.c */
